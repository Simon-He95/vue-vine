{
  "name": "@vue-vine/eslint-plugin",
  "type": "module",
  "version": "1.1.1",
  "description": "ESLint plugin for Vue Vine",
  "author": "ShenQingchuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-vine/vue-vine.git",
    "directory": "packages/eslint-plugin"
  },
  "keywords": [
    "Vue",
    "Vine",
    "eslint",
    "eslintPlugin"
  ],
  "exports": {
    ".": {
      "vine": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "worker"
  ],
  "scripts": {
    "test": "vitest",
    "test:dev": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "catalog:utils",
    "@vue-vine/eslint-parser": "workspace:*",
    "prettier": "catalog:lint-libs",
    "prettier-linter-helpers": "catalog:lint-libs",
    "synckit": "catalog:miscs"
  },
  "devDependencies": {
    "@types/eslint": "catalog:types",
    "@types/prettier-linter-helpers": "catalog:types",
    "@typescript-eslint/types": "catalog:lint-libs",
    "@typescript-eslint/typescript-estree": "catalog:lint-libs",
    "@typescript-eslint/utils": "catalog:lint-libs",
    "eslint": "catalog:lint-libs",
    "eslint-vitest-rule-tester": "catalog:lint-libs"
  }
}
