{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2023"],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
